        xmlns:local="Telas\clr-namespace:ProvaEMC"
        mc:Ignorable="d"
        Title="Prova EMC" Height="409.416" Width="400" ToolTip="Tela Início" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
