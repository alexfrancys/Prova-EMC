        <TextBlock HorizontalAlignment="Center" Text="Prova EMC" TextAlignment="Center" FontSize="25" VerticalAlignment="Center" Height="83" Width="392" Canvas.Left="2"/>

        <Button x:Name="Clientes" FontSize="16" FontWeight="Bold" Width="110" Height="200" Canvas.Left="50" Canvas.Top="100" Click="Clientes_Click" Background="LightSalmon" >Clientes</Button>

        <Button x:Name="Fornecedores" FontSize="16" FontWeight="Bold" Width="110" Height="200" Canvas.Left="232" Canvas.Top="100" Background="LightSalmon" Click="Fornecedores_Click" >Fornecedores</Button>

    </Canvas>
</Window>
